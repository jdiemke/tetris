{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "prefer-for-of": false,
        "no-console": false,
        "no-angle-bracket-type-assertion": false,
        "array-type": [
            true,
            "generic"
        ],
        "trailing-comma": false
    },
    "rulesDirectory": []
}